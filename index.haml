%section#past-events.vertically-centered-section
  .vertically-centered-content-wrapper.content-wrapper
    .fixed-title
      %img.icon{src: './images/2020/ico_flame.png'}
      %h1 Past Events
      %ul.nav
        %li.selected.ga-track{data: {tracker: 'past-events-2019'}} 2019
        |
        %li.ga-track{data: {tracker: 'past-events-2018'}} 2018

    #information-2019.past-event-wrapper
      %h2 OCCULTURE 2019
      %h4 31 October - 03 November
      %h4 Anomalie Art Club, Berlin

      .image
        %img{src: './images/events/2019/enid-closing-ritual.jpg'}
        .rights
          Photographer:
          %a{href: 'https://www.instagram.com/WarmDefeat/', target: 'blank'} Enid Valu
      .links
        %a.ga-track{href: './2019/lineup', data: {tracker: '2019-lineup'}} Lineup
        %br
        %a.download-program.ga-track{href: './2019/program.pdf', data: {tracker: 'download-program-2019'}}
          Download Program
    #information-2018.past-event-wrapper.hidden
      %h2 OCCULTURE 2018
      %h4 16-18 November
      %h4 Flutgraben e.V., Berlin

      .image
        %img{src: './images/events/2018/exterior.jpg'}
      .links
        // TODO: whole lotta refactorin' to do there
        %a.ga-track{href: './2018/lineup', data: {tracker: '2018-lineup'}} Lineup
        %br
        %a.download-program.ga-track{href: './2018/program.pdf', data: {tracker: 'download-program-2018'}}
          Download Program

